{
    "include": [
        "./src"
    ],
    "exclude": [
        "./src/_meta.ts"
    ],
    "compilerOptions": {
        "target": "ES2016",
        "module": "AMD",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "noUncheckedIndexedAccess": false,
        "skipLibCheck": true,
        "alwaysStrict": false,
        "outFile": "./dist/compiled.js"
    }
}